@{
    ViewData["Title"] = "Du lịch";
}
<link href="~/css/Service/Style.css" rel="stylesheet" />

<div style="height:70px;" ></div>
@model KhanhHoaTravel.Models.ViewModels.PlaceListViewModel

<div style="margin:0px 0px 50px 0px">
    <div class="place_container">
        @foreach (var p in Model.tblPlaceDeltail)
        {
            <partial name="EterPlaceTemplate" model="p" />
        }
    </div>
    <div page-model="@Model.PagingInfo" page-action="Index" page-classes-enabled="true"
         page-class="btn" page-class-normal="btn-outline-dark"
         page-class-selected="btn-primary" class="btn-group pull-right m-1">
    </div>
</div>
